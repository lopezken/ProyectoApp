<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="269dp"
        android:layout_height="495dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="80dp"
        android:contentDescription="@string/descripcion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.672"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.342"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/Test_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="140dp"
        android:fontFamily="@font/chewy"
        android:text="@string/nombre_logo2"
        android:textColor="@color/color_lauch"
        android:textSize="46sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.436"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/text_tr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="156dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/chewy"
        android:text="@string/nombre_logo1_1"
        android:textColor="#FF0000"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/Test_textView"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txt_via"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/chewy"
        android:text="@string/nombre_logo1_2"
        android:textColor="#FFEB3B"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/Test_textView"
        app:layout_constraintStart_toEndOf="@+id/text_tr" />

    <TextView
        android:id="@+id/txt_a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/chewy"
        android:text="@string/nombre_logo1_3"
        android:textColor="#4CAF50"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/Test_textView"
        app:layout_constraintStart_toEndOf="@+id/txt_via" />

</androidx.constraintlayout.widget.ConstraintLayout>